{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prebuild": "node -e \"process.env.NODE_ENV === 'production' ? require('./scripts/netlify-prebuild.js') : require('./scripts/pre-check.js')\" && node -e \"process.env.NODE_ENV === 'production' ? require('./scripts/netlify-image-convert.js') : require('./scripts/convertImagesToWebP.js')\"",
    "postbuild": "node -e \"process.env.NODE_ENV === 'production' ? require('./scripts/netlify-postbuild.js') : (require('./scripts/extractCriticalCss.js') && require('./scripts/generateSitemap.js'))\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "ANALYZE=true webpack --mode production",
    "start": "react-scripts start",
    "optimize-images": "node scripts/convertImagesToWebP.js",
    "extract-critical-css": "node scripts/extractCriticalCss.js",
    "generate-sitemap": "node scripts/generateSitemap.js",
    "check-hooks": "node scripts/ensure-react-loading.js",
    "pre-check": "node scripts/pre-check.js",
    "netlify-build": "node scripts/netlify-prebuild.js && node scripts/netlify-image-convert.js && npm run build && node scripts/netlify-postbuild.js"
  },
